{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./build/static/ && rm -rf ./build/file/download/*.xlsx && rm -rf ./build/file/upload/*.xlsx",
    "clean_log": "rm -rf ./log/*.log",
    "dev": "npm run clean && concurrently \"npm run c_start\" \"npm run s_start\" \"node ./config/openBrowser\"",
    "build": "npm run c_build",
    "start": "npm run clean && concurrently \"npm run c_build\" \"forever start -l ./log -o ./log/console.log -e ./log/err.log -a server/app.js\"",
    "stop": "forever stopall",
    "init": "npm run init_sql | npm run doc",
    "test": "node scripts/test.js test",
    "init_sql": "node ./server/init/index.js",
    "doc": "cd ./build/AppDoc && gitbook build",
    "s_start": "nodemon --harmony ./server/app.js",
    "c_start": "webpack --watch --config ./config/webpack.dev.config.js",
    "c_build": "webpack --config ./config/webpack.prod.config.js"
  },
  "dependencies": {
    "@antv/data-set": "^0.10.2",
    "antd": "^3.13.0",
    "axios": "^0.18.0",
    "bizcharts": "^3.4.5",
    "crypto": "^1.0.1",
    "forever": "^0.15.3",
    "jsonwebtoken": "^8.5.0",
    "koa": "^2.6.2",
    "koa-jwt": "^3.5.1",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "mime": "^2.4.0",
    "moment": "^2.24.0",
    "mysql": "^2.16.0",
    "mz": "^2.7.0",
    "nprogress": "^0.2.0",
    "nunjucks": "2.4.2",
    "react": "^16.7.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0",
    "sequelize": "^4.42.0",
    "validator": "^10.11.0",
    "@babel/polyfill": "^7.0.0",
    "eventsource-polyfill": "^0.9.6",
    "xlsx": "^0.14.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "autoprefixer": "7.1.6",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^2.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.1",
    "ejs": "^2.6.1",
    "file-loader": "^3.0.1",
    "happypack": "^5.0.1",
    "koa-body": "^4.0.8",
    "koa-bodyparser": "^4.2.1",
    "koa-logger": "^3.2.0",
    "koa-views": "^6.1.5",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "npm": "^6.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
